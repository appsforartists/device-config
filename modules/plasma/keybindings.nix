{lib, ...}: let
  plasmaKeyNames = {
    super = "Meta";
    option = "Alt";
    return = "Enter";
    delete = "Del";
    escape = "Esc";
    page_up = "PgUp";
    page_down = "PgDown";
  };

  shiftedPlasmaKeyNames = {
    "`" = "~";
    "/" = "?";
    "\\" = "|";
    "[" = "{";
    "]" = "}";
    tab = "Backtab";
    equal = "+";
    minus = "_";
  };

  toPlasmaKeybinding = keybinding: let
    shiftSearch = map (keyName: "shift+${keyName}") (lib.attrNames shiftedPlasmaKeyNames);
    shiftReplace = lib.attrValues shiftedPlasmaKeyNames;
    lowerCasePlasmaKeybinding = lib.replaceStrings shiftSearch shiftReplace keybinding;
  in
    lib.concatMapStringsSep "+"
    (keyName: lib.toSentenceCase (plasmaKeyNames.${keyName} or keyName))
    (lib.splitString "+" lowerCasePlasmaKeybinding);

  toPlasmaKeybindings = keybinding:
    if builtins.isList keybinding
    then map toPlasmaKeybinding keybinding
    else toPlasmaKeybinding keybinding;

  plasmaBindings = lib.mapAttrsRecursive (_: value: toPlasmaKeybindings value) lib.my.keybindings;

  formatForConfig = value:
    if builtins.isList value
    then lib.concatStringsSep "; " value
    else value;
